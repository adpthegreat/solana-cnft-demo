{
  "compilerOptions": {
     "outDir": "dist",
     "target": "es2022",
     "moduleResolution": "Bundler",
     "module": "ES2022",
     "esModuleInterop": true,
     "forceConsistentCasingInFileNames": false,
     "strict": true,
     "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
